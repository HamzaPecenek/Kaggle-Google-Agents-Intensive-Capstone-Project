e.adk.runners import Runner\n\n\nMODEL = \"gemini-2.0-flash\"\n\n\n# -------------------------------\n# Task Intake Agent\n# -------------------------------\n\ntask_intake_agent = LlmAgent(\n    name=\"task_intake_agent\",\n    model=MODEL,\n    description=(\n        \"You ONLY respond if the user is adding a study task. \"\n        \"If they mention adding, scheduling, duration, deadline, or a new assignment, \"\n        \"you extract the task and call save_task using the tool. \"\n        \"If it's not a task, output nothing.\"\n    ),\n    tools=[save_task_tool],\n)\n\n\n# -------------------------------\n# Study Planner Agent\n# -------------------------------\n\nstudy_planner_agent = LlmAgent(\n    name=\"study_planner_agent\",\n    model=MODEL,\n    description=(\n        'You ONLY respond if the user wants a \"plan\", \"schedule\", or \"study plan\". '\n        \"First call load_tasks using the tool, then organize them into a clean daily or weekly plan. \"\n        \"If it's not planning related, output nothing.\"\n    ),\n    tools=[load_tasks_tool],\n)\n\n\n# -------------------------------\n# Reflection Agent\n# -------------------------------\n\nreflection_agent = LlmAgent(\n    name=\"reflection_agent\",\n    model=MODEL,\n    description=(\n        \"You ONLY respond if the user is reporting progress or completion. \"\n        \"Ask what was completed, load tasks, update them using save_task, and return a reflection summary. \"\n        \"If the message is not about progress/completion, output nothing.\"\n    ),\n    tools=[save_task_tool, load_tasks_tool],\n)\n\n\n# -------------------------------\n# ParallelAgent = Multi-agent\n# -------------------------------\n\ncoordinator = ParallelAgent(\n    name=\"focusmate_multi_agent\",\n    agents=[\n        task_intake_agent,\n        study_planner_agent,\n        reflection_agent\n    ]\n)\n\n\n# -------------------------------\n# Runner Setup\n# -------------------------------\n\nsession = InMemorySessionService()\nrunner = Runner(agent=coordinator, session=session)\n\nprint(\"Parallel multi-agent system loaded successfully!\")\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-12-01T13:39:40.450137Z","iopub.execute_input":"2025-12-01T13:39:40.450487Z","iopub.status.idle":"2025-12-01T13:39:40.477432Z","shell.execute_reply.started":"2025-12-01T13:39:40.450465Z","shell.execute_reply":"2025-12-01T13:39:40.475944Z"}},"outputs":[{"traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mValidationError\u001b[0m                           Traceback (most recent call last)","\u001b[0;32m/tmp/ipykernel_47/3993311458.py\u001b[0m in \u001b[0;36m<cell line: 0>\u001b[0;34m()\u001b[0m\n\u001b[1;32m     65\u001b[0m \u001b[0;31m# -------------------------------\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m     66\u001b[0m \u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0;32m---> 67\u001b[0;31m coordinator = ParallelAgent(\n\u001b[0m\u001b[1;32m     68\u001b[0m     \u001b[0mname\u001b[0m\u001b[0;34m=\u001b[0m\u001b[0;34m\"focusmate_multi_agent\"\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m     69\u001b[0m     agents=[\n","\u001b[0;32m/usr/local/lib/python3.11/dist-packages/pydantic/main.py\u001b[0m in \u001b[0;36m__init__\u001b[0;34m(self, **data)\u001b[0m\n\u001b[1;32m    248\u001b[0m         \u001b[0;31m# `__tracebackhide__` tells pytest and some other tools to omit this function from tracebacks\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m    249\u001b[0m         \u001b[0m__tracebackhide__\u001b[0m \u001b[0;34m=\u001b[0m \u001b[0;32mTrue\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0;32m--> 250\u001b[0;31m         \u001b[0mvalidated_self\u001b[0m \u001b[0;34m=\u001b[0m \u001b[0mself\u001b[0m\u001b[0;34m.\u001b[0m\u001b[0m__pydantic_validator__\u001b[0m\u001b[0;34m.\u001b[0m\u0